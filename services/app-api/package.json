    "@types/jwt-encode": "^1.0.0",
    "@types/md5": "^2.3.2",
    "@types/node": "*",
    "@types/object-hash": "^3.0.3",
    "@types/prompts": "^2.4.9",
    "aws-lambda": "^1.0.7",
    "aws-sdk-client-mock": "^4.0.0",
    "dotenv": "^16.4.5",

  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "globals": {
      "isolatedModules": true
    },
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "handlers/**/*.{ts,tsx}",
      "utils/**/*.{ts,tsx}",
      "storage/**/*.{ts,tsx}",
      "!utils/constants/*",
      "!utils/testing/*",
      "!utils/types/*"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "#node-web-compat": "./node-web-compat-node.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/utils/testing/setupJest.ts"
    ]
  }

{
  "name": "ui-src",
  "description": "",
  "version": "1.0.0",
  "license": "CC0-1.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "coverage": "jest --coverage",
    "test": "jest --setupFiles dotenv/config",
    "watchTest": "jest --setupFiles dotenv/config --watch",
    "a11y": "pa11y --runner axe --runner htmlcs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.9",
    "@cmsgov/design-system": "^3.8.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^2.9.11",
    "@vitejs/plugin-react": "^4.3.0",
    "aws-amplify": "^5.3.19",
    "date-fns": "^2.26.0",
    "date-fns-tz": "^1.3.8",
    "dompurify": "^2.5.3",
    "framer-motion": "^4",
    "html-react-parser": "^3.0.16",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^28.0.0",
    "launchdarkly-react-client-sdk": "^3.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.51.5",
    "react-icons": "^4.12.0",
    "react-router-dom": "6.23.1",
    "react-uuid": "^1.0.3",
    "sass": "^1.77.2",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2",
    "yup": "^0.32.11",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^27.0.2",
    "@types/jest-axe": "^3.5.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.2",
    "babel": "^6.23.0",
    "babel-jest": "^26.6.3",
    "history": "^5.3.0",
    "jest-axe": "^6.0.0",
    "jest-launchdarkly-mock": "^2.0.2",
    "pa11y": "^6.2.3",
    "pa11y-runner-htmlcs": "^2.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-helmet": "^6.1.0",
    "serverless-cloudfront-invalidate": "^1.5.0",
    "serverless-s3-sync": "^3.2.0",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17"
  },
  "jest": {
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "!src/components/index.ts*",
      "src/routes/**/*.{ts,tsx}",
      "!src/routes/index.tsx",
      "!src/types/index.ts*",
      "src/utils/**/*.{ts,tsx}",
      "!src/utils/testing/*",
      "!src/utils/index.ts*",
      "!src/utils/**/index.ts*"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg|mp4|webm|wav|mp3|m4a)$": "<rootDir>/src/utils/testing/mockAsset.ts"
    },
    "setupFiles": [
      "jest-launchdarkly-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/utils/testing/setupJest.tsx"
    ],
    "globals": {
      "_env_": {}
    },
    "testEnvironment": "jest-environment-jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
